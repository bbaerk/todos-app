{
  "name": "todos-app",
  "description": "Handy TODOs app!",
  "version": "1.0.0",
  "author": "Parker McMullin <parker.mcmullin01@gmail.com>",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic yarn dev",
    "build": "yarn routify -- -b && rollup -c",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "build:docker": "yarn build && ./scripts/docker/docker-build.sh",
    "deploy:now": "cd scripts/now && yarn deploy",
    "deploy:netlify": "cd scripts/netlify && yarn deploy",
    "rollup": "rollup -cw",
    "routify": "routify -D"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-load-config": "^2.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.6.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "spassr": "^1.0.3",
    "svelte-preprocess": "^3.9.10",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@sveltech/routify": "^1.7.12",
    "@tailwindcss/ui": "^0.3.0",
    "svelte": "^3.20.1",
    "tailwindcss": "^1.4.6"
  }
}
